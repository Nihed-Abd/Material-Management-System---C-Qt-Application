QPushButton {
    background-color: #0EABB8; /* Bleu clair pour représenter l'eau */
    border: 2px solid #0EABB8; /* Bordure bleue */
    color: #ffffff; /* Texte blanc */
    padding: 15px 25px; /* Espacement intérieur */
    font-size: 18px; /* Taille de la police */
    font-weight: bold; /* Police en gras */
    border-radius: 20px; /* Bordure arrondie */
    outline: none; /* Supprime la bordure de focus */
    position: relative; /* Position relative pour les pseudo-éléments */
    overflow: hidden; /* Cache le contenu débordant */
    transition: background-color 0.3s, color 0.3s; /* Transition des couleurs */
}

/* Style au survol du bouton */
QPushButton:hover {
    background-color: #03a9fc; /* Fond devient blanc */
    color: #0EABB8; /* Texte devient bleu clair */
}

/* Style au clic du bouton */
QPushButton:pressed {
    background-color: #1e4c6f; /* Bleu foncé lors du clic */
    color: #f8f8f8; /* Texte blanc lors du clic */
}

/* Animation pour le pseudo-élément :after */
QPushButton::after {
    content: ''; /* Contenu vide */
    position: absolute; /* Position absolue */
    width: 100%; /* Largeur à 100% */
    height: 100%; /* Hauteur à 100% */
    background-color: rgba(255, 255, 255, 0.3); /* Fond semi-transparent */
    top: 50%; /* Positionnement au centre vertical */
    left: 50%; /* Positionnement au centre horizontal */
    border-radius: 50%; /* Bordure arrondie */
    transform: translate(-50%, -50%) scale(0); /* Réduit l'échelle à 0 */
    transition: transform 0.5s ease; /* Transition de l'échelle */
}

/* Animation au survol du bouton */
QPushButton:hover::after {
    transform: translate(-50%, -50%) scale(2); /* Augmente l'échelle à 2 */
}</string>
